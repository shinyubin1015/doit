{
	"name": "doit",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"lint": "eslint .",
		"preview": "npm run build && vite preview",
		"deploy": "npm run build",
		"deploy:wrangler": "npm run build && wrangler deploy",
		"schema:generate": "npx wrangler d1 export doit --remote --output=./database.sql",
		"schema:apply": "wrangler d1 execute doit --file=./database.sql"
	},
	"dependencies": {
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-router-dom": "^7.13.0"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "^1.21.2",
		"@eslint/js": "^9.33.0",
		"@types/react": "^19.1.10",
		"@types/react-dom": "^19.1.7",
		"@vitejs/plugin-react": "^5.0.0",
		"eslint": "^9.33.0",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.20",
		"globals": "^16.3.0",
		"vite": "^7.1.2",
		"wrangler": "^4.61.1"
	}
}
